{
  "manifest_version": 2,

  "name": "Code Scrapper",
  "version": "1.0",
  "description": "Scrap code from current tab",

  "browser_action": {
    "default_icon": "twitter-icon.png"
  },

  "background": {
    "scripts": ["event.js"],
    "persistent": false
  },
  
  "permissions": ["<all_urls>"]
}